{
  "name": "pokenative",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:dev": "jest --watchAll --passWithNoTest",
    "test:coverage": "jest --watchAll=false --collect-coverage --passWithNoTest"
  },
  "dependencies": {
    "expo": "~47.0.12",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "@types/react-native-dotenv": "^0.2.0",
    "react-native-dotenv": "^3.4.2",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/jest-native": "^5.3.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-native": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.55.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "jest": "^27.5.1",
    "jest-expo": "^47.0.1"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@fortawesome/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "./src/jest.setup.ts",
      "@testing-library/jest-native/extend-expect"
    ]
  }
}
